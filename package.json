{
  "name": "yn-p1",
  "displayName": "YN P1",
  "description": "元年 P1 插件",
  "version": "0.7.0",
  "publisher": "yuanian",
  "engines": {
    "vscode": "^1.35.0"
  },
  "icon": "images/yuannian128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wenlz123/yn-p1-vsc"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "workspaceContains:vue.config.js"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createYnComponent",
        "title": "添加元年标准组件"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createYnComponent",
          "group": "yuanian",
          "when": "explorerResourceIsFolder"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.createYnComponent",
        "key": "alt+shift+c"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "sh build.sh --watch",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "publish_major": "vsce publish major",
    "publish_minor": "vsce publish minor",
    "publish_patch": "vsce publish patch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.35.0",
    "glob": "^7.1.5",
    "mocha": "^10.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vsce": "^2.5.3",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "pretty": "^2.0.0",
    "shelljs": "^0.8.3"
  }
}
